  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "homepage": "https://yet-another-react-lightbox.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/igordanchenko/yet-another-react-lightbox.git"
  },
  "bugs": {
    "url": "https://github.com/igordanchenko/yet-another-react-lightbox/issues"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf dist",
    "start": "npm-run-all clean --parallel \"build:* -- -w\"",
    "build": "npm-run-all clean build:scss build:css build:js build:fixup",
    "build:scss": "sass src --no-source-map",
    "build:css": "postcss src/*.css src/**/*.css --base src -d dist -u autoprefixer cssnano --no-map",
    "build:js": "rollup -c",
    "build:fixup": "node fixup-dist.js",
    "lint": "eslint .",
    "test": "npm-run-all test:unit test:types",
    "test:unit": "vitest run --coverage",
    "test:types": "node test/types",
    "dev": "vite --config dev/vite.config.ts"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "chokidar": "^3.5.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.0",
    "rollup-plugin-dts": "^6.1.0",
    "sass": "^1.69.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6"
  },
  "keywords": [
    "react",
    "image",
    "photo",
    "lightbox",
    "react lightbox",
    "react image lightbox",
    "react photo lightbox"
  ]
